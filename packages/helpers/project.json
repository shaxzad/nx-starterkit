{
  "name": "helpers",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/helpers/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/helpers",
        "tsConfig": "packages/helpers/tsconfig.lib.json",
        "packageJson": "packages/helpers/package.json",
        "main": "packages/helpers/src/auth.service.ts",
        "assets": ["packages/helpers/*.md"],
        "updateBuildableProjectDepsInPackageJson": true
      }
    }
  }
}
